{
  "name": "johnlindquist.com",
  "description": "johnlindquist.com",
  "version": "1.0.0",
  "author": "John Lindquist",
  "bugs": {
    "url": "https://github.com/johnlindquist/johnlindquist.com/issues"
  },
  "dependencies": {
    "dropbox": "^2.5.13",
    "gatsby": "^1.9.146",
    "gatsby-link": "^1.6.32",
    "gatsby-plugin-feed": "^1.3.15",
    "gatsby-plugin-google-analytics": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.12",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sharp": "^1.6.24",
    "gatsby-remark-autolink-headers": "^1.4.11",
    "gatsby-remark-code-repls": "^1.0.8",
    "gatsby-remark-copy-linked-files": "^1.5.25",
    "gatsby-remark-emoji": "0.0.1",
    "gatsby-remark-images": "^1.5.36",
    "gatsby-remark-prismjs": "^1.2.11",
    "gatsby-remark-responsive-iframe": "^1.4.16",
    "gatsby-remark-smartypants": "^1.4.10",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-remark": "^1.7.25",
    "gatsby-transformer-sharp": "^1.6.16",
    "lodash": "^4.17.4",
    "react-responsive-grid": "^0.3.4",
    "tailwindcss": "^0.4.0",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typeface-roboto-slab": "0.0.44"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "npm-watch": "^0.3.0",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": ["gatsby"],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "scripts": {
    "dev":
      "nodemon --watch styles.css --exec \"npm run tailwind\" | gatsby develop",
    "lint":
      "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format":
      "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "npm run tailwind && gatsby build",
    "deploy": "npm run build && cd public && now && now alias",
    "fix-semi":
      "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "tailwind": "tailwind build styles.css -c ./tailwind.js -o ./src/output.css"
  }
}
